{
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "injected.js"
            ],
            "matches": [
                "*://*.random.org/*"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "manifest_version": 2,
    "name": "Selector random.org",
    "options_page": "options.html",
    "permissions": [
        "*://*.random.org/*",
        "storage"
    ],
    "version": "1.0.0"
}